.organic-background.data-v-b9f10b13 {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  z-index: 1;
  pointer-events: none;
  /* 确保背景不会拦截点击事件 */
}
.shape.data-v-b9f10b13 {
  position: absolute;
  border-radius: 50%;
  filter: blur(80rpx);
  /* 40px -> 80rpx */
  animation: float-b9f10b13 infinite ease-in-out alternate;
  opacity: 1;
  /* 确保形状是可见的 */
}
.shape-1.data-v-b9f10b13 {
  width: 800rpx;
  /* 400px -> 800rpx */
  height: 800rpx;
  /* 400px -> 800rpx */
  top: -400rpx;
  /* -200px -> -400rpx */
  right: -200rpx;
  /* -100px -> -200rpx */
}
.shape-2.data-v-b9f10b13 {
  width: 600rpx;
  /* 300px -> 600rpx */
  height: 600rpx;
  /* 300px -> 600rpx */
  top: 200rpx;
  /* 100px -> 200rpx */
  left: -300rpx;
  /* -150px -> -300rpx */
}
.shape-3.data-v-b9f10b13 {
  width: 700rpx;
  /* 350px -> 700rpx */
  height: 700rpx;
  /* 350px -> 700rpx */
  bottom: -240rpx;
  /* -120px -> -240rpx */
  right: 100rpx;
  /* 50px -> 100rpx */
}
.shape-4.data-v-b9f10b13 {
  width: 500rpx;
  /* 250px -> 500rpx */
  height: 500rpx;
  /* 250px -> 500rpx */
  bottom: 100rpx;
  /* 50px -> 100rpx */
  left: 200rpx;
  /* 100px -> 200rpx */
}
@keyframes float-b9f10b13 {
0% {
    transform: translate(0, 0);
}
50% {
    transform: translate(40rpx, 40rpx);
    /* 20px -> 40rpx */
}
100% {
    transform: translate(-40rpx, -40rpx);
    /* -20px -> -40rpx */
}
}